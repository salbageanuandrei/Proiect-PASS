@model IEnumerable<Proiect_PASS_New_1.Models.Student>

@{
  ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
  @Html.ActionLink("Create New", "Create")
</p>
<p>
  @Html.ActionLink("Afisare prognoza studenti", "SubmitPrognoza")
</p>
<p>
  @Html.ActionLink("PreziceZiuaReturn", "PreziceZiuaReturn")
</p>

<table class="table">
  <tr>
    <th>
      @Html.DisplayNameFor(model => model.NrMatricol)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.Nume)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.Prenume)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.Medie)
    </th>
    <th></th>
  </tr>

  @foreach (var item in Model)
  {
    <tr>
      <td>
        @Html.DisplayFor(modelItem => item.NrMatricol)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.Nume)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.Prenume)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.Medie)
      </td>
      <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id }) |
        @Html.ActionLink("PrognozaMedie", "Submit", new { id = item.Id }) |
      </td>
    </tr>
  }

</table>
